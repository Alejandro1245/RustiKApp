<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GraficasVentas">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/altFondoVentasG"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/Superior"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/almacen_ventas__1" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/Superior"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:contentDescription="@string/altLineaVentas"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/almacen_ventas__1" />

        <ImageView
            android:id="@+id/Logo03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="@string/altLogo01"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logo_pizzeria_sin_fondo" />

        <TextView
            android:id="@+id/textFiltros"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="@string/textBalanceTotal"
            android:textColor="@color/blancorusti"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.13"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Logo03" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="@string/textBalanceDinero"
            android:textColor="@color/blancorusti"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.215"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textFiltros" />

        <ImageButton
            android:id="@+id/imageButton16"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="52dp"
            android:backgroundTint="@android:color/transparent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:srcCompat="@drawable/componente_156__3" />

        <ImageButton
            android:id="@+id/imageButton14"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:background="@android:color/transparent"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageButton16"
            app:srcCompat="@drawable/ic_rect_ngulo_155" />

        <ImageView
            android:id="@+id/FondoDatosInven01"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:contentDescription="@string/altFondoDatosInventario"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageButton14"
            app:srcCompat="@drawable/ic_trazado_66" />

        <TextView
            android:id="@+id/MesesLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Labelmeses"
            android:textColor="@color/negroRusti"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageButton14"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageButton14" />

        <ImageView
            android:id="@+id/FondoDatosInven02"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:contentDescription="@string/altFondoDatosInventario"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.107"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/FondoDatosInven01"
            app:srcCompat="@drawable/ic_trazado_66" />

        <ImageView
            android:id="@+id/FondoDatosInven03"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:contentDescription="@string/altFondoDatosInventario"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/FondoDatosInven02"
            app:srcCompat="@drawable/ic_trazado_66" />

        <ImageView
            android:id="@+id/imgIconVentas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:contentDescription="@string/altIconVentas"
            app:layout_constraintBottom_toBottomOf="@+id/FondoDatosInven01"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/FondoDatosInven01"
            app:srcCompat="@drawable/ventasicon" />

        <TextView
            android:id="@+id/TextVenta1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="3dp"
            android:text="@string/LabelVenta1"
            android:textColor="@color/negroRusti"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textDesVenta1"
            app:layout_constraintStart_toEndOf="@+id/imgIconVentas"
            app:layout_constraintTop_toTopOf="@+id/FondoDatosInven01"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textDesVenta1"
            android:layout_width="215dp"
            android:layout_height="35dp"
            android:layout_marginStart="12dp"
            android:layout_marginBottom="20dp"
            android:text="@string/LabelDescripVenta1"
            android:textColor="@color/blancorusti"
            app:layout_constraintBottom_toBottomOf="@+id/FondoDatosInven01"
            app:layout_constraintStart_toEndOf="@+id/imgIconVentas" />

        <TextView
            android:id="@+id/textCostoVenta1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/LabelCostoVenta1"
            android:textColor="@color/blancorusti"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/FondoDatosInven01"
            app:layout_constraintEnd_toEndOf="@+id/FondoDatosInven01"
            app:layout_constraintHorizontal_bias="0.612"
            app:layout_constraintStart_toEndOf="@+id/textDesVenta1"
            app:layout_constraintTop_toBottomOf="@+id/TextVenta1"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/imgIconVentas2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:contentDescription="@string/altIconVentas"
            app:layout_constraintBottom_toBottomOf="@+id/FondoDatosInven02"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/FondoDatosInven02"
            app:layout_constraintVertical_bias="0.454"
            app:srcCompat="@drawable/ventasicon" />

        <TextView
            android:id="@+id/textVenta2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            android:text="@string/LabelVenta2"
            android:textColor="@color/negroRusti"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textDesVenta2"
            app:layout_constraintStart_toEndOf="@+id/imgIconVentas2"
            app:layout_constraintTop_toTopOf="@+id/FondoDatosInven02" />

        <TextView
            android:id="@+id/textDesVenta2"
            android:layout_width="215dp"
            android:layout_height="35dp"
            android:layout_marginStart="12dp"
            android:layout_marginBottom="20dp"
            android:text="@string/LabelDescripVenta1"
            android:textColor="@color/blancorusti"
            app:layout_constraintBottom_toBottomOf="@+id/FondoDatosInven02"
            app:layout_constraintStart_toEndOf="@+id/imgIconVentas2" />

        <TextView
            android:id="@+id/textCostoVenta2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:text="@string/LabelCostoVenta2"
            android:textColor="@color/blancorusti"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/FondoDatosInven02"
            app:layout_constraintEnd_toEndOf="@+id/FondoDatosInven02"
            app:layout_constraintHorizontal_bias="0.263"
            app:layout_constraintStart_toEndOf="@+id/textDesVenta2"
            app:layout_constraintTop_toBottomOf="@+id/textVenta2"
            app:layout_constraintVertical_bias="0.0" />

    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>